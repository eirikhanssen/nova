<script type="text/javascript">
    window.onload = function(){
            $("#show_summary_trigger").click(function(){
		$(".publication_summary").show();		   
		$("#show_summary_trigger").hide();
            });
	$("#close_summary_trigger").click(function(){
		$(".publication_summary").hide();		   
		$("#show_summary_trigger").show();
	})
    }
</script>


		          <!-- Main area content: END -->
</div>
			</div>
		</div><!--end:frameWrapper-->
</div>        <!-- Extra area: START -->
    
