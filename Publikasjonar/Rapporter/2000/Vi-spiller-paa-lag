 	 	var nhutmaprevious;
 	 	var nhutmalatest;
 	 	var nhutmanumber;
 	 	{
			if (document.cookie != '')
			var nhcookie = document.cookie.split('; ');
			var x;
			for (x in nhcookie)
			{
				if (nhcookie[x].substring(0, 6) == "__utma")
				{
					nhutmaparts = nhcookie[x].split('.');
					nhutmaid = nhutmaparts[1];
					nhutmafirst = nhutmaparts[2];
					nhutmaprevious = nhutmaparts[3];
					nhutmalatest = nhutmaparts[4];
					nhutmanumber = nhutmaparts[5];
				}
			}
 	 	 }
	</script>

